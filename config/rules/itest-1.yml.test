id: '00022'
name: NDR日志ELT规则
vendor: NDR
description: 搜集NDR日志
worker: 1
inputs:
    - Random:
        from: 1
        to: 100
        max_messages: 1
filters:
    - Add:
       fields:
           observer_vendor: 未来智安
           sum: '{{ add 1 2 }}'
           contains: '{{ contains "nihao" "ni" }}'
           ip: '{{ ipv4ToInt "192.168.0.1" }}'
    - Convert:
          fields:
              ip:
                  to: int
              message:
                  to: int
                  setto_if_nil: 0
outputs:
    - Stdout: {}